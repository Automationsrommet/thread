module kniv(){
minkowski(){
rotate([-2,0,0])
linear_extrude(0.5)
polygon([[-16.5/2,0],[16.5/2,0],[0,16.5*sin(60)]]);
sphere(0.3);
}
}

module flaske(){
translate([-50,0,-20])
difference(){
cylinder(150,50,50,center=false);
cylinder(301,49,49,center=true);
}
}

module guide(){
rotate([10,0,0])
for(i=[-1:2:1]){
translate([i*5,4,0])
resize([8,10,28])
sphere(10);
translate([i*5,4,-14])
cube([6,8,21],center=true);
}
}

module stav(){
translate([-50,0,-20])
cylinder(300,8,8,center=false);
}
