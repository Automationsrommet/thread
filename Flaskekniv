module justeringplass(){
translate([0,-13,-36.5])
rotate([-20,0,0])
{
cylinder(30,4.5,4.5,center=false);
cylinder(22,16,16,center=true);
for(i=[0:10:360*7])
rotate([4,0,i])
translate([4,0,i/(9*9)])
rotate([90,0,-30])
sphere(2.5,$fn=3);
translate([0,0,30])sphere(4.5);
}
}


module justering(){
translate([0,-13,-36.5])
rotate([-20,0,0])
{
cylinder(30,4,4,center=false);
cylinder(3,15,15,center=true);
for(i=[0:10:360*7])
rotate([4,0,i])
translate([4,0,i/(9*9)])
rotate([90,0,-30])
sphere(2,$fn=3);
translate([0,0,25])sphere(4);
}
}

module skruer(){
translate([63,110,-30])
rotate([0,-90,0])
skrue();
translate([63,90,-30])
rotate([0,-100,0])
skrue();
translate([-53,107,-30])
rotate([0,105,0])
skrue();
translate([-53,93,-30])
rotate([0,105,0])
skrue();
translate([-50,0,-42])
skrue();
}

module skrue(){
cylinder(40,1.8,1.8,center=false);
cylinder(10,6,1,center=false);
for(i=[0:10:360*7])
rotate([4,0,i])
translate([1,0,i/(9*9)])
rotate([90,0,-30])
sphere(2,$fn=3);
}

module arm(){
difference(){
union(){
translate([55,100,95])
rotate([0,90,0])
cylinder(11,14.5,14.5,center=true,$fn=100);
hull(){
translate([76,100,95])
rotate([30,0,0])
translate([0,0,-100])
rotate([0,90,0])
cylinder(6,8,10,center=true,$fn=5);
translate([66,100,95])
rotate([0,90,0])
cylinder(10,22,15,center=true,$fn=6);
}
}
translate([38,100,95])
rotate([0,90,0])
for(j=[0:120:359])
rotate([0,0,j])
translate([00,10,0])
skrue();
translate([76,100,95])
rotate([30,0,0])
translate([0,0,-100])
rotate([0,90,0])
cylinder(200,2.5,2.5,center=true,$fn=5);
}
}

module sveiv(){
intersection(){
difference(){
translate([99,100,95])
rotate([30,0,0])
translate([0,0,-100])
resize([44,20,25])
sphere(20,$fn=12);

translate([76,100,95])
rotate([30,0,0])
translate([0,0,-100])
rotate([0,90,0])
cylinder(200,2,2,center=true,$fn=5);
}
translate([102,140,0])cube(40,center=true);
}
}

